<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="240.0" prefWidth="440.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.icet.cims.controller.TrueFalseController">
   <children>
      <Label fx:id="txt" layoutX="44.0" layoutY="72.0" text="Do you want to create database? " textFill="#000000d3">
         <font>
            <Font name="Arial Bold" size="22.0" />
         </font>
      </Label>
      <Label layoutX="177.0" layoutY="116.0" textFill="#000000d3">
         <font>
            <Font name="Arial Bold" size="22.0" />
         </font>
      </Label>
      <Button fx:id="btn_true" layoutX="144.0" layoutY="142.0" mnemonicParsing="false" onAction="#btn_TrueAction" prefHeight="30.0" prefWidth="66.0" text="True" textFill="#198339eb">
         <font>
            <Font name="Arial Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="btn_false" defaultButton="true" layoutX="231.0" layoutY="142.0" mnemonicParsing="false" onAction="#btn_FalseAction" prefHeight="30.0" prefWidth="66.0" text="False" textFill="#a42323cc">
         <font>
            <Font name="Arial Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
