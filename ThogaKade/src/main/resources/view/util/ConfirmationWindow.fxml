<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="262.0" prefWidth="337.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.icet.cims.controller.util.ConfirmationWindowController">
   <children>
      <StackPane fx:id="stackpane" layoutX="33.0" layoutY="64.0" prefHeight="125.0" prefWidth="271.0" />
      <Button fx:id="btn_yes" layoutX="111.0" layoutY="200.0" mnemonicParsing="false" onAction="#btn_yesAction" text="Yes" textFill="#10802a">
         <font>
            <Font name="Arial Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="btn_no" layoutX="191.0" layoutY="200.0" mnemonicParsing="false" onAction="#btn_noAction" prefHeight="27.0" prefWidth="47.0" text="No" textFill="#b52424">
         <font>
            <Font name="Arial Bold" size="15.0" />
         </font>
      </Button>
      <Label fx:id="lbl_heading" layoutX="31.0" layoutY="21.0" text="User Confirmation">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
